<table class="table">
<thead class="thead-dark">
  <tr>
    <th>日時</th>
    <th>メニュー</th>
    <th>距離</th>
    <th colspan="3"></th>
  </tr>
</thead>
<tbody>
  <% @blogs_for_pagenation.order("start_time DESC").each do |blog| %>
    <tr>
      <td><%= blog.start_time.strftime("%Y-%m-%d") %></td>
      <td><%= blog.content %></td>
      <td><%= blog.distance %>　Km</td>
      <td><%= link_to '詳細', blog_path(blog.id) %></td>
      <td><%= link_to '編集', edit_blog_path(blog.id) %></td>
      <td><%= link_to '削除',blog_path(blog.id), method: :delete, data: { confirm: '削除してよろしいですか。' } %></td>
    </tr>
  <% end %>
</tbody>
</table>
<%= paginate @blogs_for_pagenation, :remote => true, theme: "twitter-bootstrap-4" %>         
